    <sql>WITH rows AS&#x28;&#xd;&#xa;INSERT INTO osc.tb_projeto&#x28;id_osc, tx_nome_projeto, ft_nome_projeto, cd_status_projeto, ft_status_projeto, dt_data_inicio_projeto, ft_data_inicio_projeto, dt_data_fim_projeto, ft_data_fim_projeto,&#xd;&#xa;			 tx_link_projeto, ft_link_projeto,nr_valor_total_projeto, ft_valor_total_projeto, cd_abrangencia_projeto, ft_abrangencia_projeto, tx_descricao_projeto,ft_descricao_projeto, bo_oficial&#x29;&#xd;&#xa;select id_osc, &#x27;&#x3f;&#x27;,&#x27;&#x3f;&#x27;,&#x3f;,&#x27;&#x3f;&#x27;,to_date&#x28;&#x27;&#x3f;&#x27;,&#x27;YYYY&#x2f;MM&#x2f;DD&#x27;&#x29; as dt_ini,&#x27;&#x3f;&#x27;,to_date&#x28;&#x27;&#x3f;&#x27;,&#x27;YYYY&#x2f;MM&#x2f;DD&#x27;&#x29; as dt_fim,&#x27;&#x3f;&#x27;,&#x27;&#x3f;&#x27;,&#x27;&#x3f;&#x27;,&#x3f;,&#x27;&#x3f;&#x27;,case when length&#x28;&#x27;&#x3f;&#x27;&#x29; &#x3d; 0 then null else to_number&#x28;&#x27;&#x3f;&#x27;,&#x27;99999999&#x27;&#x29; end,&#x27;&#x3f;&#x27;,&#x27;&#x3f;&#x27;,&#x27;&#x3f;&#x27;,false&#xd;&#xa;from osc.tb_osc&#xd;&#xa;where cd_identificador_osc &#x3d; &#x3f;&#xd;&#xa;RETURNING id_projeto&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;INSERT INTO osc.tb_localizacao_projeto&#x28;id_projeto, id_regiao_localizacao_projeto, ft_regiao_localizacao_projeto, bo_oficial,tx_nome_regiao_localizacao_projeto&#x29;&#xd;&#xa;select id_projeto, case when length&#x28;&#x27;&#x3f;&#x27;&#x29; &#x3d; 0 then 0 else to_number&#x28;&#x27;&#x3f;&#x27;,&#x27;99999999&#x27;&#x29; end as id_regiao, &#x27;&#x3f;&#x27;, false as bo_oficial, &#x27;Sem Descri&#xe7;&#xe3;o&#x27; as nome_regiao&#xd;&#xa;from rows&#xd;&#xa;&#xd;&#xa;</sql>
